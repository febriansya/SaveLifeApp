<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.SaveLifeApp" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.SaveLifeApp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.SaveLifeApp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.SaveLifeApp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="tab_text" parent="Base.TextAppearance.AppCompat">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textSize">30sp</item>
    </style>

    <style name="tab_text123" parent="Base.TextAppearance.AppCompat">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="CornerRadisu">
        <item name="cornerSize">10%</item>
    </style>
</resources>